{
  "plugins": ["react", "prettier"],
  "rules": {
    "prettier/prettier": "error",
    "import/no-anonymous-default-export": "off",
    "egegeg": "off"
  },
  "parser":"@babel/eslint-parser",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "es6": true,
    "node": true
  }
}
